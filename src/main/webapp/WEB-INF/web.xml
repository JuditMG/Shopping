<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Shopping</servlet-name>
  	<display-name>Shopping</display-name>
  	<description></description>
  	<servlet-class>Shopping.Shopping</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShoppingServlet</servlet-name>
  	<display-name>ShoppingServlet</display-name>
  	<description></description>
  	<servlet-class>ShoppingServlet.ShoppingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Shopping</servlet-name>
  	<url-pattern>/Shopping</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShoppingServlet</servlet-name>
  	<url-pattern>/ShoppingServlet</url-pattern>
  </servlet-mapping>
</web-app>
